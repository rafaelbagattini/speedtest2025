<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>7.4</version>
    <template_groups>
        <template_group>
            <uuid>a571c0d144b14fd4a87a9d9b2aa9fcd6</uuid>
            <name>Templates/Applications</name>
        </template_group>
    </template_groups>
    <templates>
        <template>
            <uuid>df1ad9a6af4949e5956900c03de73177</uuid>
            <template>Speedtest 2026</template>
            <name>Speedtest 2026</name>
            <groups>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <items>
                <item>
                    <uuid>28460f84ca6e4fd88f0cd7852328a5a1</uuid>
                    <name>Download</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.download</key>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <trends>90d</trends>
                    <units>bps</units>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.download.bandwidth</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <parameters>
                                <parameter>8</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <uuid>4c70250fda674b769f7ef3f00fe565e3</uuid>
                            <expression>last(/Speedtest 2026/speedtest.download,#1)&lt;=({$VELOCIDADE.CONTRATADA} * 0.4)</expression>
                            <name>Internet Lenta!!</name>
                            <priority>WARNING</priority>
                            <description>Velocidade de Download abaixo de 40% do contratado.</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>10ca3c127774465793e2b0129ed2c764</uuid>
                    <name>Provedor</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.isp</key>
                    <history>1d</history>
                    <value_type>TEXT</value_type>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.isp</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                </item>
                <item>
                    <uuid>b3b6b44827114bb7b34e20f02f7ad3c3</uuid>
                    <name>Ping</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.ping</key>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <trends>90d</trends>
                    <units>ms</units>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.ping.latency</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                </item>
                <item>
                    <uuid>a2c6b558e9cf478f9a1eca03b7e86f05</uuid>
                    <name>Print</name>
                    <type>TRAP</type>
                    <key>speedtest.print</key>
                    <history>1d</history>
                    <value_type>TEXT</value_type>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED</type>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <uuid>d8db6333abb543b2b9369f3a0ad657ed</uuid>
                    <name>Teste de Velocidade</name>
                    <type>TRAP</type>
                    <key>speedtest.resultado</key>
                    <history>1d</history>
                    <value_type>TEXT</value_type>
                </item>
                <item>
                    <uuid>51d1287a0fe842f2861686cc57f52090</uuid>
                    <name>Hora do teste</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.timestamp</key>
                    <history>1d</history>
                    <value_type>CHAR</value_type>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.timestamp</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <parameters>
                                <parameter>const date = new Date(value);
const timestampSeconds = Math.floor(date.getTime() / 1000);
return ('0' + date.getDate()).slice(-2) + &quot;/&quot; + (&quot;0&quot; + (date.getMonth() + 1)).slice(-2) + &quot;/&quot; + date.getFullYear() + &quot; &quot; + ('0' + date.getHours()).slice(-2) + &quot;:&quot; + ('0' + date.getMinutes()).slice(-2)</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                </item>
                <item>
                    <uuid>0804810a5d404bb5a6b39bd4e1dc7cbd</uuid>
                    <name>Upload</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.upload</key>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <trends>90d</trends>
                    <units>bps</units>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.upload.bandwidth</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <parameters>
                                <parameter>8</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                </item>
                <item>
                    <uuid>13e37d848c3c437fb6178808dbb08ad2</uuid>
                    <name>URL</name>
                    <type>DEPENDENT</type>
                    <key>speedtest.url.print</key>
                    <history>1d</history>
                    <value_type>TEXT</value_type>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.result.url</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>REGEX</type>
                            <parameters>
                                <parameter>^(.*)$</parameter>
                                <parameter>\1.png</parameter>
                            </parameters>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED</type>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>speedtest.resultado</key>
                    </master_item>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$VELOCIDADE.CONTRATADA}</macro>
                    <value>700000000</value>
                    <description>Velocidade de download contratado em bytes</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
